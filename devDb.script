CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE BUSCA_TWITTER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,QUANTIDADE INTEGER NOT NULL,PALAVRA VARCHAR(255) NOT NULL,IDIOMA VARCHAR(255) NOT NULL,ULTIMO_TWEET BIGINT)
CREATE MEMORY TABLE PERFIL(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,AUTHORITY VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_49 UNIQUE(AUTHORITY))
CREATE MEMORY TABLE PERFIL_PEOPLE(PERFIL_ID BIGINT NOT NULL,USUARIO_ID BIGINT NOT NULL,PRIMARY KEY(PERFIL_ID,USUARIO_ID),CONSTRAINT FKEE66862AA9E7A5A FOREIGN KEY(PERFIL_ID) REFERENCES PERFIL(ID))
CREATE MEMORY TABLE PERMISSAO(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,URL VARCHAR(255) NOT NULL,CONFIG_ATTRIBUTE VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_55 UNIQUE(URL))
CREATE MEMORY TABLE USUARIO(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,ENABLED BOOLEAN NOT NULL,PASSWD VARCHAR(255) NOT NULL,USER_REAL_NAME VARCHAR(255) NOT NULL,USERNAME VARCHAR(255) NOT NULL,EMAIL_SHOW BOOLEAN NOT NULL,EMAIL VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_59 UNIQUE(USERNAME))
CREATE MEMORY TABLE UMESP_TWEET(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,TEXTO VARCHAR(255) NOT NULL,USUARIO VARCHAR(255) NOT NULL,DATA TIMESTAMP NOT NULL,AVATAR_USUARIO VARCHAR(255) NOT NULL)
ALTER TABLE PERFIL_PEOPLE ADD CONSTRAINT FKEE66862E94612DA FOREIGN KEY(USUARIO_ID) REFERENCES USUARIO(ID)
ALTER TABLE BUSCA_TWITTER ALTER COLUMN ID RESTART WITH 6
ALTER TABLE PERFIL ALTER COLUMN ID RESTART WITH 3
ALTER TABLE PERMISSAO ALTER COLUMN ID RESTART WITH 6
ALTER TABLE USUARIO ALTER COLUMN ID RESTART WITH 3
ALTER TABLE UMESP_TWEET ALTER COLUMN ID RESTART WITH 6278
